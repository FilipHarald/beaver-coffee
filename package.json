{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "vue": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "buildvue": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "./node_modules/.bin/babel src -s -D -d dist --presets env && npm run -s buildvue",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "engines": {
    "node": "7.10.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "mongoose": "^4.9.9",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "laravel-mix": "^0.11.4",
    "moment": "^2.18.1",
    "nodemon": "^1.9.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3"
  }
}
